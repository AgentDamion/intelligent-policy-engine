{
  "name": "aicomply-policy-engine",
  "version": "0.1.0",
  "description": "Intelligent Policy Engine for Pharmaceutical AI Governance",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec \"node server-unified.js\" --watch server-unified.js",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "test": "node test-integration.js",
    "test:watch": "jest --watch",
    "setup:db": "echo \"Run fix-rls-policies.sql in your Supabase SQL editor\"",
    "setup:agents": "echo \"Agent setup placeholder for Week 4\"",
    "start": "node server-unified.js",
    "fix:rls": "echo \"Please run fix-rls-policies.sql in your Supabase SQL editor\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
