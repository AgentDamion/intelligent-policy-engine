{
  "name": "aicomplyr-intelligence",
  "version": "2.0.0",
  "description": "AICOMPLYR AI Governance Platform - Comprehensive AI governance for pharmaceutical enterprises and marketing agencies",
  "main": "server-railway.js",
  "keywords": [
    "ai-governance",
    "pharmaceutical",
    "marketing-agencies",
    "policy-compliance",
    "agentic-ai",
    "human-override",
    "real-time-sync"
  ],
  "author": "AICOMPLYR Team",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aicomplyr/aicomplyr-intelligence.git"
  },
  "homepage": "https://aicomplyr.io",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server-railway.js",
    "dev": "nodemon server-railway.js",
    "railway": "node server-railway.js",
    "local": "node server.js",
    "migrate": "node database/migrate.js",
    "migrate:rollback": "node database/migrate.js rollback",
    "migrate:status": "node database/migrate.js status",
    "supabase:migrate": "node supabase/run-migrations-direct.js run-all",
    "supabase:validate": "node supabase/validate-deployment.js",
    "add-admin-audit": "node database/add-admin-audit-table.js",
    "seed": "node database/seed-sample-orgs-agencies.js",
    "seed:hierarchical": "node database/seed-hierarchical-data.js",
    "seed:templates": "node database/seed-policy-templates.js",
    "seed:common": "node database/seed-common-ai-tools-and-templates.js",
    "test": "node test-agency-api.js",
    "test-health": "node test-health-connection.js",
    "test-railway": "node test-railway.js",
    "test:orchestration": "node test-enhanced-orchestration.js",
    "test:override": "node test-override-system.js",
    "build:ui": "cd ui && npm run build",
    "start:ui": "cd ui && npm start",
    "install:all": "npm install && cd ui && npm install",
    "setup": "npm run migrate && npm run seed"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "auth0": "^4.27.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-oauth-server": "^2.0.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lucide-react": "^0.526.0",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.2",
    "pg": "^8.11.1",
    "react": "^19.1.0",
    "sanitize-html": "^2.17.0",
    "uuid": "^9.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "supertest": "^7.1.4"
  }
}
